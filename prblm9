public class Student
{
	int rollno;
	String SName;
	String Saddress;
void accept()
{
	rollno = 123;
	SName = "Gautam";
	Saddress = "vns";
}
void display()
{
	System.out.println(rollno);
	System.out.println(SName);
	System.out.println(Saddress);
}
}

	class Example
	{
	public static void main(String[] args)
	{
		Student S1=new Student();
		S1.accept();
		S1.display();
	}
}

